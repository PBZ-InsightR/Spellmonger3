<?xml version="1.0" encoding="UTF-8"?>

<?xml-stylesheet href="@style.css" type="text/css" ?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.insightr.Controller.ControllerMenu">
    <children>
        <Pane fx:id="mainMenu" prefHeight="700.0" prefWidth="900.0">
            <children>
                <Button fx:id="Score" layoutX="352.0" layoutY="400.0" mnemonicParsing="false" onAction="#goToScore" text="Scores" prefWidth="90" />
                <Button fx:id="Play" layoutX="500.0" layoutY="400.0" mnemonicParsing="false" onAction="#goToPlay" text="Play" prefWidth="90" />
                <Label fx:id="label_login1" layoutX="365.0" layoutY="265.0" prefHeight="20.0" prefWidth="60.0" text="Player 1" textAlignment="CENTER" />
                <TextField fx:id="login1" layoutX="425.0" layoutY="265.0" prefHeight="5.0" prefWidth="100.0" />
                <Label fx:id="label_login2" layoutX="365.0" layoutY="321.0" prefHeight="20.0" prefWidth="60.0" text="Player 2" textAlignment="CENTER" />
                <TextField fx:id="login2" layoutX="425.0" layoutY="320.0" prefHeight="5.0" prefWidth="100.0" />

                <Label fx:id="label_IA" layoutX="550.0" layoutY="300.0" prefHeight="20.0" prefWidth="60.0" text="IA level" textAlignment="CENTER" />
                <CheckBox fx:id="IA_LV1" layoutX="550.0" layoutY="323.0" mnemonicParsing="false" onAction="#isIA_LV1" />
                <CheckBox fx:id="IA_LV2" layoutX="580.0" layoutY="323.0" mnemonicParsing="false" onAction="#isIA_LV2" />
            </children>
        </Pane>
    </children>
</GridPane>
